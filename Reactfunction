class ParentFunction extends React.Component {
  constructor(props) {
    super(props);
  }
  render() {
    return (
  <div>
   <h1>React Component</h1>
   <childfunction/>
   </div>
    );
  }
};
class childfunction extends React.Component {
  constructor(props) {
    super(props);
  }
  render() {

  return (
      <p>I am the loving child of the parent </p>
    );
  };
  }
ReactDOM.render(ParentFunction, document.getElementById(""));
